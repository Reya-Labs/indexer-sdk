{
  "name": "indexer-sdk",
  "version": "1.0.1",
  "description": "Client for the Voltz indexers living on Google Cloud",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/types",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prebuild": "yarn code-quality:check && rm -rf dist",
    "build": "rollup -c --bundleConfigAsCjs",
    "ts:check": "yarn ts:check-types && yarn ts:circular-check",
    "ts:check-types": "tsc --noEmit",
    "ts:circular-check": "dpdm --no-output --no-tree --no-warning --exit-code circular:1 -T ./src/index.ts",
    "lint:check": "eslint '{src,tests}/**/*.ts'",
    "lint:fix": "eslint --fix '{src,tests}/**/*.ts'",
    "prettier:check": "prettier --check './{src,tests}/**/*.ts'",
    "prettier:fix": "prettier --write './{src,tests}/**/*.ts'",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "test": "jest --silent",
    "test:coverage": "yarn test --coverage",
    "generate:coverage-badges": "npx istanbul-badges-readme --silent",
    "code-quality:check": "npx concurrently 'yarn ts:check' 'yarn prettier:check' 'yarn lint:check --quiet'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voltz-Protocol/indexer-sdk.git"
  },
  "author": "ssbarbee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Voltz-Protocol/indexer-sdk/issues"
  },
  "homepage": "https://github.com/Voltz-Protocol/indexer-sdk#readme",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@swc/core": "1.3.29",
    "@swc/jest": "0.2.24",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "babel-loader": "^8.3.0",
    "concurrently": "7.6.0",
    "dpdm": "3.11.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "istanbul-badges-readme": "1.8.4",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "prettier": "2.4.1",
    "redis-cli": "2.1.2",
    "redis-server": "1.2.2",
    "rollup": "3.11.0",
    "rollup-plugin-bundle-size": "1.0.3",
    "rollup-plugin-dts": "5.1.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "19.0.5",
    "semantic-release-cli": "5.4.4",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^6.2.0",
    "@google-cloud/storage": "^6.9.5",
    "axios": "^1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "5.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "ioredis": "^5.3.1",
    "rate-limit-redis": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "README.md"
          ],
          "message": "chore: Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "engines": {
    "node": ">= 16.17.0",
    "yarn": ">= 1.22.0",
    "npm": "please-use-yarn"
  },
  "transform": {
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  }
}
